					<xs:sequence>
						<xs:element name="levelCrossing" type="eng:LevelCrossing" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="baliseGroup" minOccurs="0" maxOccurs="1">
