			<xs:element name="supportedOnBoardEquipment" type="opp:SupportedOnBoardEquipment" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="operationalConsist" type="opp:OperationalConsist"/>
