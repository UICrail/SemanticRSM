		<xs:restriction base="xs:string">
			<xs:enumeration value="orientationUknown"/>
			<xs:enumeration value="orientationSameAsTrainUnit"/>
			<xs:enumeration value="orientationInverseToTrainUnit"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EventState">
		<xs:restriction base="xs:string">
			<xs:enumeration value="created"/>
			<xs:enumeration value="updated"/>
			<xs:enumeration value="removed"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OccupationReason">
		<xs:restriction base="xs:string">
			<xs:enumeration value="shorterLTrain"/>
			<xs:enumeration value="toCommLoss"/>
			<xs:enumeration value="systemInit"/>
			<xs:enumeration value="ttdOccupation"/>
			<xs:enumeration value="ttdFree"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DPSGState">
		<xs:restriction base="xs:string">
			<xs:enumeration value="ready"/>
			<xs:enumeration value="processing"/>
			<xs:enumeration value="disturbed"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DpsDrivability">
		<xs:restriction base="xs:string">
			<xs:enumeration value="full"/>
			<xs:enumeration value="none"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RestrictionState">
		<xs:restriction base="xs:string">
			<xs:enumeration value="created"/>
			<xs:enumeration value="removed"/>
		</xs:restriction>
	</xs:simpleType>
