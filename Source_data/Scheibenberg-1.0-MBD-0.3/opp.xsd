					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="movementRestrictions" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="movementRestriction" type="opp:MovementRestriction" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
