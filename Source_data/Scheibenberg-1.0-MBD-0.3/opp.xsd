					<xs:sequence>
						<xs:element name="operationalPlanMovement" type="opp:OperationalPlanMovement" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="operationalPlanRestrictions" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="operationalPlanRestriction" type="opp:OperationalPlanRestriction" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="operationalPlanWarningMeasures" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="operationalPlanWarningMeasure" type="opp:OperationalPlanWarningMeasure" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="operationalTrafficPlan" type="opp:OperationalTrafficPlan"/>
