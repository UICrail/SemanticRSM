			<xs:enumeration value="fullSupervision"/>
			<xs:enumeration value="standBy"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MovementEventAction">
		<xs:restriction base="xs:string">
			<xs:enumeration value="passedEvent"/>
			<xs:enumeration value="stoppedAccurateAtEvent"/>
			<xs:enumeration value="stoppedWithOvershootAtEvent"/>
			<xs:enumeration value="stoppedWithUndershootAtEvent"/>
			<xs:enumeration value="departedFromEvent"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AtoState">
		<xs:restriction base="xs:string">
			<xs:enumeration value="unknown"/>
			<xs:enumeration value="CO"/>
			<xs:enumeration value="NA"/>
			<xs:enumeration value="AV"/>
			<xs:enumeration value="RE"/>
			<xs:enumeration value="EG"/>
			<xs:enumeration value="DE"/>
			<xs:enumeration value="FA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DoorState">
		<xs:restriction base="xs:string">
			<xs:enumeration value="doorStateUnknown"/>
			<xs:enumeration value="doorStateBlocked"/>
			<xs:enumeration value="doorStateAvailable"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PantographState">
		<xs:restriction base="xs:string">
			<xs:enumeration value="pantographBlocked"/>
			<xs:enumeration value="pantographLowered"/>
			<xs:enumeration value="pantographRaised"/>
			<xs:enumeration value="pantographStateUnknown"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DoorSide">
		<xs:restriction base="xs:string">
			<xs:enumeration value="doorSideLeft"/>
			<xs:enumeration value="doorSideRight"/>
			<xs:enumeration value="doorSideBoth"/>
			<xs:enumeration value="doorNoneSide"/>
			<xs:enumeration value="doorSideUnknown"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ActiveState">
		<xs:restriction base="xs:string">
			<xs:enumeration value="activeState"/>
			<xs:enumeration value="inactiveState"/>
			<xs:enumeration value="undefinedState"/>
		</xs:restriction>
	</xs:simpleType>
