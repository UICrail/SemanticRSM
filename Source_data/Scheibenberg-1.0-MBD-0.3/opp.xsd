		<xs:attribute name="issuedAt" type="xs:dateTime" use="required"/>
		<xs:attribute name="configurationDataVersionRef" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:element name="operationalPlanMovement" type="opp:OperationalPlanMovement"/>
