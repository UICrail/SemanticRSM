			<xs:element name="etcsSystemVersions" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="etcsSystemVersion" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="value" type="xs:string" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="anyClasses" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="anyClass" type="opp:AnyClass" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="trainProtectionType" type="opp:TrainProtectionType" use="required"/>
	</xs:complexType>
	<xs:element name="supportedOnBoardEquipment" type="opp:SupportedOnBoardEquipment"/>
	<xs:complexType name="AnyClass">
		<xs:sequence/>
		<xs:attribute name="className" type="xs:string" use="required"/>
		<xs:attribute name="content" type="xs:base64Binary" use="required"/>
	</xs:complexType>
	<xs:element name="anyClass" type="opp:AnyClass"/>
	<xs:complexType name="MovementRestriction">
		<xs:sequence>
			<xs:element name="restrictedPath" type="infra:LinkedPath" minOccurs="1" maxOccurs="1"/>
			<xs:element name="specificRestriction" type="opp:SpecificMovementRestriction" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="movementRestriction" type="opp:MovementRestriction"/>
