		<xs:choice>
			<xs:element name="maxSpeed">
				<xs:complexType>
					<xs:attribute name="value" type="xs:unsignedInt" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="adhesionCategory">
				<xs:complexType>
					<xs:attribute name="value" type="opp:AdhesionCategory" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="maxCurrent">
				<xs:complexType>
					<xs:attribute name="value" type="xs:unsignedInt" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="atoInhibition">
				<xs:complexType>
					<xs:attribute name="value" type="xs:boolean" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="dasInhibition">
				<xs:complexType>
					<xs:attribute name="value" type="xs:boolean" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="excludedDPSGroupsForFlankProtection" type="opp:DPSGroupsList"/>
		</xs:choice>
