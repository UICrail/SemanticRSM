        <rdfs:subClassOf rdf:resource="http://cdm.ovh/rsm/topology/topology.owl#NetElement"/>
        <owl:disjointWith rdf:resource="http://cdm.ovh/rsm/topology/topology.owl#NonlinearElement"/>
