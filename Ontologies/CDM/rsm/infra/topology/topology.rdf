<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc11="http://purl.org/dc/elements/1.1/"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:ns0="https://vocab.org/vann/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#">

  <owl:Ontology rdf:about="https://cdm.ovh/rsm/topology/topology">
    <owl:versionIRI rdf:resource="https://cdm.ovh/rsm/topology/1.0rc3/topology"/>
    <owl:imports rdf:resource="http://cdm.ovh/rsm/adapters/geosparql_adapter/geosparql_adapter"/>
    <dc11:abstract xml:lang="en">Simplified RSM topology; relations between net elements ports only available as object properties (no reification).RSM is a member of the CDM (Conceptual Data Model), under development under the ERJU MOTIONAL project.Some properties that are most likely to be used are chained properties (equivalent to derived associations in UML).Superclasses for time-dependency are yet to be chosen and introduced.</dc11:abstract>
    <dc11:contributor xml:lang="en">Pierre Tane, UIC</dc11:contributor>
    <dc11:creator xml:lang="en">Airy Magnien, UIC</dc11:creator>
    <dc11:date xml:lang="en">24 February 2025</dc11:date>
    <dc11:relation xml:lang="en">MOTIONAL project: https://rail-research.europa.eu/eu-rail-projects/ ONTORAIL project: https://ontorail.org/RSM GitHub: https://github.com/UICrail/SemanticRSM</dc11:relation>
    <dc11:title xml:lang="en">RSM topology</dc11:title>
    <dc11:type xml:lang="en">RDF/OWL ontology</dc11:type>
    <dc:license xml:lang="en">EUPL 1.2; see https://interoperable-europe.ec.europa.eu/sites/default/files/custom-page/attachment/eupl_v1.2_en.pdf</dc:license>
    <owl:versionInfo>1.0rc3</owl:versionInfo>
    <ns0:preferredNamespacePrefix>topo</ns0:preferredNamespacePrefix>
    <ns0:preferredNamespaceURI xml:lang="en">http://cdm.ovh/rsm/topology/topology#</ns0:preferredNamespaceURI>
  </owl:Ontology>

