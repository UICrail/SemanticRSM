  <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/eddy/connectedWith">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/eddy/connectedWithTransitive">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://cdm.ovh/rsm/eddy/Port"/>
        <rdfs:range rdf:resource="http://cdm.ovh/rsm/eddy/Port"/>
        <rdfs:label xml:lang="en">connected with (transitive)</rdfs:label>
        <skos:definition xml:lang="en">transitive closure of connectedWith.</skos:definition>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>

    <rdfs:label xml:lang="en">connected with</rdfs:label>
    <skos:definition xml:lang="en">Relation between a port on a net element and the asjacent port on another net element. Geographic locations of related ports may not coincide, either because of uncertainties or rounding errors, or by construction (e.g. case of tracks joined via a turntable).</skos:definition>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/eddy/navigableTo">
    <rdfs:subPropertyOf rdf:resource="http://cdm.ovh/rsm/eddy/navigableToTransitive"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/topology/topology#navigableFrom">
        <rdfs:label xml:lang="en">navigable from</rdfs:label>
        <skos:definition xml:lang="en">inverse of navigable to</skos:definition>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:label xml:lang="en">navigable to</rdfs:label>
    <skos:definition xml:lang="en">Relation between a port on a net element from which rolling stock can exit, to another port on an adjacent net element from which the same rolling stock can then exit, in one continuous movement (without direction reversal).</skos:definition>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/eddy/navigableToTransitive">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/topology/topology#navigableFromTransitive">
        <rdfs:label xml:lang="en">navigable from (transitive)</rdfs:label>
        <skos:definition xml:lang="en">inverse of navigable to (transitive)</skos:definition>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:domain rdf:resource="http://cdm.ovh/rsm/eddy/Port"/>
    <rdfs:range rdf:resource="http://cdm.ovh/rsm/eddy/Port"/>
    <rdfs:label xml:lang="en">navigable to (transitive)</rdfs:label>
    <skos:definition xml:lang="en">transitive closure of navigableTo.</skos:definition>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/eddy/onElement">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <owl:inverseOf rdf:resource="http://cdm.ovh/rsm/eddy/port"/>
    <rdfs:label xml:lang="en">on element</rdfs:label>
    <skos:definition xml:lang="en">inverse of port, port0, port1</skos:definition>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/eddy/port">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdfs:domain rdf:resource="http://cdm.ovh/rsm/topology/topology#NetElement"/>
    <rdfs:range rdf:resource="http://cdm.ovh/rsm/eddy/Port"/>
    <rdfs:label xml:lang="en">port</rdfs:label>
    <skos:definition xml:lang="en">Associated ports with net elements. This is an inverse functional property, meaning that a port can belong to at most one net element (if a port is found to belong to two net elements, the inference is that those two elements are the same).</skos:definition>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/eddy/port1">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://cdm.ovh/rsm/eddy/port"/>
    <rdfs:domain rdf:resource="http://cdm.ovh/rsm/eddy/LinearElement"/>
    <owl:propertyDisjointWith>
      <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/eddy/port_0">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://cdm.ovh/rsm/eddy/port"/>
        <rdfs:label xml:lang="en">port 0</rdfs:label>
        <skos:definition xml:lang="en">First (or start) port of some net element. Allows to orient linear elements. Can also be used on others, with undefined semantics.</skos:definition>
      </owl:ObjectProperty>
    </owl:propertyDisjointWith>

    <rdfs:label>port 1</rdfs:label>
    <skos:definition xml:lang="en">second (or end) port on a linear element.</skos:definition>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/topology/topology#leadsTo">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/topology/topology#leadsToTranstitive">
        <owl:propertyChainAxiom>
          <rdf:Description>
            <rdf:first rdf:resource="http://cdm.ovh/rsm/eddy/port"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://cdm.ovh/rsm/eddy/navigableToTransitive"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://cdm.ovh/rsm/eddy/onElement"/>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:propertyChainAxiom>

        <rdfs:label xml:lang="en">leads to (transtitive)</rdfs:label>
        <skos:definition xml:lang="en">relation between non-adjacent net elements, where rolling stock may leave the first and reach the second without reversing travel direction.</skos:definition>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>

    <rdfs:domain rdf:resource="http://cdm.ovh/rsm/topology/topology#NetElement"/>
    <rdfs:range rdf:resource="http://cdm.ovh/rsm/topology/topology#NetElement"/>
    <owl:propertyChainAxiom>
      <rdf:Description>
        <rdf:first rdf:resource="http://cdm.ovh/rsm/eddy/port"/>
        <rdf:rest>
          <rdf:Description>
            <rdf:first rdf:resource="http://cdm.ovh/rsm/eddy/navigableTo"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://cdm.ovh/rsm/eddy/onElement"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </rdf:rest>

      </rdf:Description>
    </owl:propertyChainAxiom>

    <rdfs:label xml:lang="en">leads to</rdfs:label>
    <skos:definition xml:lang="en">relation between two adjacent net elements (i.e. having connected ports), where rolling stock can start moving from the first and arrive on the second without reversing travel direction.</skos:definition>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/topology/topology#nonNavigableFrom">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://cdm.ovh/rsm/topology/topology#nonNavigableTo">
        <rdfs:domain rdf:resource="http://cdm.ovh/rsm/eddy/Port"/>
        <rdfs:range rdf:resource="http://cdm.ovh/rsm/eddy/Port"/>
        <rdfs:label xml:lang="en">non navigable to</rdfs:label>
        <skos:definition xml:lang="en">Expresses that there is no navigability link between the related ports. Given the OWA (open world assumption), a navigability not expressed does not mean &quot;non navigability&quot;. Consequently, non-navigability should be explicitly stated, as it is important knowledge.</skos:definition>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:label xml:lang="en">non navigable from</rdfs:label>
    <skos:definition xml:lang="en">inverse of non navigable to</skos:definition>
  </owl:ObjectProperty>

  <owl:Class rdf:about="http://cdm.ovh/rsm/adapters/geosparql_adapter#Feature">
  </owl:Class>

  <owl:Class rdf:about="http://cdm.ovh/rsm/eddy/LinearElement">
    <rdfs:subClassOf rdf:resource="http://cdm.ovh/rsm/topology/topology#NetElement"/>
    <owl:disjointWith>
      <owl:Class rdf:about="http://cdm.ovh/rsm/eddy/NonlinearElement">
        <rdfs:subClassOf rdf:resource="http://cdm.ovh/rsm/topology/topology#NetElement"/>
        <rdfs:label xml:lang="en">Nonlinear element</rdfs:label>
        <skos:definition xml:lang="en">Net element with any number of ports except two. Used for expressing connexities and navigabilities outside a complex network area (e.g. a yard or a station).</skos:definition>
      </owl:Class>
    </owl:disjointWith>

    <rdfs:label xml:lang="en">Linear element</rdfs:label>
    <skos:definition xml:lang="en">Class of linear elements, i.e. all those elements that have two extremities (ports) and a meaningful measure between those extremities, such that the linear element can be considered a metric space (in the mathematical sense).</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="http://cdm.ovh/rsm/eddy/ListedPort">
    <rdfs:subClassOf rdf:resource="http://cdm.ovh/rsm/eddy/Port"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/list#OWLList">
      </owl:Class>
    </rdfs:subClassOf>

    <rdfs:label xml:lang="en">Listed port</rdfs:label>
    <skos:definition xml:lang="en">A port in an ordered list (with successors; no contents needed). See also OWLList ontology.</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="http://cdm.ovh/rsm/eddy/Port">
    <rdfs:subClassOf rdf:resource="http://cdm.ovh/rsm/adapters/geosparql_adapter#Feature"/>
    <owl:disjointWith rdf:resource="http://cdm.ovh/rsm/topology/topology#NetElement"/>
    <rdfs:label>Port</rdfs:label>
    <skos:definition xml:lang="en">Any place, at the boundary of a net element, that can be an entry and/or exit point for rolling stock.</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="http://cdm.ovh/rsm/topology/topology#BoundaryElement">
    <rdfs:subClassOf rdf:resource="http://cdm.ovh/rsm/eddy/LinearElement"/>
    <rdfs:label xml:lang="en">Boundary element</rdfs:label>
    <skos:definition xml:lang="en">Linear element with length = 0. It still has two distinct port, allowing to express connectivity and navigability. To be used at the borders of networks managed by different entities.</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="http://cdm.ovh/rsm/topology/topology#NetElement">
    <rdfs:subClassOf rdf:resource="http://cdm.ovh/rsm/adapters/geosparql_adapter#Feature"/>
    <rdfs:label xml:lang="en">Net element</rdfs:label>
    <skos:definition xml:lang="en">Superclass for net elements (network elements)</skos:definition>
  </owl:Class>

</rdf:RDF>
